@model EisppReportFilterVM
@using IOWebApplication.Infrastructure.Models.ViewModels.Eispp
@{
    ViewData["Title"] = "Брои събития към ЕИСПП";
}
<div class="row">
    <div class="col-md-12 col-lg-12">
        <form id="formEdit" asp-action="FridayReport" method="post">
            <div class="row">
                <div class="col-md-3 col-lg-2">
                    @Html.EditorFor(x => x.DateFrom, "DateTimeWithTime")
                </div>
                <div class="col-md-3 col-lg-2">
                    @Html.EditorFor(x => x.DateTo, "DateTimeWithTime")
                </div>
                <div class="col-md-3">
                    <input type="submit" value="Зареди" id="ReportButton" class="btn btn-success" />
                </div>

            </div>
        </form>
    </div>
</div>
