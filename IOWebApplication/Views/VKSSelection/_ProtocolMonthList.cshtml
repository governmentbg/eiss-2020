@model List<VksSelectionOneMonthListVM>
@using IOWebApplication.Infrastructure.Models.ViewModels
@using IOWebApplication.Infrastructure.Constants
@using IOWebApplication.Infrastructure.Extensions


<table width="100%" class="no-border protokol-table" >
    @if (Model.Count == 1)
    {

        <tr><td> <partial name="_ProtocolMonth" model="@Model[0]" /></td><td> </td></tr>


    }

    @if (Model.Count == 2)
    {
        <tr><td> <partial name="_ProtocolMonth" model="@Model[0]" /></td><td> <partial name="_ProtocolMonth" model="@Model[1]" /></td></tr>


    }

    @if (Model.Count == 3)
    {
        <tr><td> <partial name="_ProtocolMonth" model="@Model[0]" /></td><td> <partial name="_ProtocolMonth" model="@Model[2]" /></td></tr>
        <tr><td> <partial name="_ProtocolMonth" model="@Model[1]" /></td><td> </td></tr>

    }
    @if (Model.Count == 4)
    {
        <tr><td> <partial name="_ProtocolMonth" model="@Model[0]" /></td><td> <partial name="_ProtocolMonth" model="@Model[2]" /></td></tr>
        <tr><td> <partial name="_ProtocolMonth" model="@Model[1]" /></td><td> <partial name="_ProtocolMonth" model="@Model[3]" /></td></tr>

    }

    @if (Model.Count == 5)
    {
        <tr><td> <partial name="_ProtocolMonth" model="@Model[0]" /></td><td> <partial name="_ProtocolMonth" model="@Model[3]" /></td></tr>
        <tr><td> <partial name="_ProtocolMonth" model="@Model[1]" /></td><td> <partial name="_ProtocolMonth" model="@Model[4]" /></td></tr>
        <tr><td> <partial name="_ProtocolMonth" model="@Model[2]" /></td><td> </td></tr>
    }
    @if (Model.Count == 6)
    {
        <tr><td  > <partial name="_ProtocolMonth" model="@Model[0]" /></td><td> <partial name="_ProtocolMonth" model="@Model[3]" /></td></tr>
        <tr><td> <partial name="_ProtocolMonth" model="@Model[1]" /></td><td> <partial name="_ProtocolMonth" model="@Model[4]" /></td></tr>
        <tr><td> <partial name="_ProtocolMonth" model="@Model[2]" /></td><td> <partial name="_ProtocolMonth" model="@Model[5]" /></td></tr>
    }
</table>


