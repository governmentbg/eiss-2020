            @Html.Raw("Сумите се разделят както следва:" + " <br />");
            @foreach (var caseMoney in Model.CaseFastProcessView.PersonSum)
            {
                @Html.Raw("<div class='just'>")
                @Html.Raw(caseMoney.MoneyText + " <br />");
                @Html.Raw("</div>")
            }
        }
    }
